<?php
// Connecting to MySQL Database.
 $con = mysqli_connect("gator4159.hostgator.com", "mriquotin", "ea2v,eSOc7gb", "mriquoti_user");
 
 // Getting the received JSON into $json variable.
 $json = file_get_contents('php://input');
 
 // decoding the received JSON and store into $obj variable.
 $obj = json_decode($json,true);
 
 $username = $obj['username'];
 
 // Populate Student name from JSON $obj array and store into $S_Name.
 $scale = $obj['scale'];
 
 // Populate Student Class from JSON $obj array and store into $S_Class.
 $switches = $obj['switches'];
 $cycle_time = $obj['cycle_time'];
 // Populate Student Phone Number from JSON $obj array and store into $S_Phone_Number.
 $climb_time = $obj['climb_time'];
 
 // Populate Email from JSON $obj array and store into $S_Email.
 $auto = $obj['auto'];
 
 $cross_line_auto = $obj['cross_line_auto'];
  
 $switch_auto = $obj['switch_auto'];
 
 $vault_auto = $obj['vault_auto'];
 
 $vault = $obj['vault'];
 
 $team_power = $obj['team_power'];
 
 $cube_num = $obj['cube_num'];
 
 $team_hang = $obj['team_hang'];
 
  $team_focus = $obj['team_focus'];
 
 $team_penalties = $obj['team_penalties'];

 $dec_auto = $obj['dec_auto'];
 
 $dec_endgame = $obj['dec_endgame'];
 
 $dec_strat = $obj['dec_strat'];

 
 // Creating SQL query and insert the record into MySQL database table.
$Sql_Query = "insert into gamedata (username, scale, switches, cycle_time, climb_time, auto, cross_line_auto, switch_auto, vault_auto, vault, team_power, cube_num, team_hang, team_focus, team_penalties, dec_auto, dec_endgame, dec_strat) values ('$username','$scale','$switches','$cycle_time','$climb_time','$auto','$cross_line_auto','$switch_auto','$vault_auto','$vault','$team_power','$cube_num','$team_hang','$team_focus','$team_penalties','$dec_auto','$dec_endgame','$dec_strat')";
 
 if(mysqli_query($con,$Sql_Query)){
 
 // If the record inserted successfully then show the message.
$MSG = 'Record Successfully Inserted Into MySQL Database.' ;
 
// Converting the message into JSON format.
$json = json_encode($MSG);
 
// Echo the message.
 echo $json ;
 
 }
 else{
 
 echo 'Try Again';
 
 }
 mysqli_close($con);
?>
